<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog version="2.12" name="webkit2gtk" library="gladewebkit2gtk" domain="glade" depends="gtk+" book="WebKit2GTK">

  <glade-widget-classes>

    <glade-widget-class title="WebKit Web View" name="WebKitWebView" get-type-function="webkit_web_view_get_type" generic-name="webview">
      <set-property-function>glade_webkit_web_view_set_property</set-property-function>
      <properties>
	<property weight="1.1" save="False" name="URL" id="glade-url">
	  <parameter-spec>
	    <type>GParamString</type>
	  </parameter-spec>
	  <tooltip>The URL to load in Glade (this property will not be saved)</tooltip>
	</property>
        <property id="user-content-manager" disabled="True"/>
        <property id="web-context" disabled="True"/> 
      </properties>
    </glade-widget-class>

    <glade-widget-class toplevel="True" title="WebKit Settings" name="WebKitSettings" get-type-function="webkit_settings_get_type" generic-name="settings"/>

  </glade-widget-classes>
  
  <glade-widget-group title="WebKit2GTK+ Widgets" name="webkit2gtk">
    <glade-widget-class-ref name="WebKitWebView"/>
    <glade-widget-class-ref name="WebKitSettings"/>
  </glade-widget-group>

</glade-catalog>